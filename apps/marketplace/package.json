{
	"name": "marketplace",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"start": "NODE_ENV=production node dist/server",
		"dev": "tsx scripts/watch",
		"build": "pnpm build.client && pnpm build.render && pnpm build.server && pnpm prerender",
		"build.client": "vite build",
		"build.render": "vite build --ssr",
		"build.server": "tsx scripts/build",
		"prerender": "tsx scripts/prerender",
		"sb": "sb dev -p 6006",
		"sb.build": "sb build",
		"lint": "pnpm typecheck && pnpm eslint && pnpm stylelint",
		"typecheck": "tsc --noEmit",
		"eslint": "eslint . --ext .ts,.tsx",
		"stylelint": "stylelint client/**/*.css"
	},
	"dependencies": {
		"@fastify/static": "6.6.1",
		"@vitejs/plugin-react": "3.0.1",
		"autoprefixer": "10.4.13",
		"dotenv": "16.0.3",
		"envalid": "7.3.1",
		"esbuild": "0.17.0",
		"fastify": "4.11.0",
		"history": "5.3.0",
		"hyperid": "3.1.0",
		"isbot": "3.6.5",
		"pino": "8.8.0",
		"pino-pretty": "9.1.1",
		"postcss": "8.4.21",
		"postcss-custom-media": "9.0.1",
		"postcss-import": "15.1.0",
		"postcss-nested": "6.0.0",
		"postcss-pxtorem": "6.0.0",
		"postcss-reporter": "7.0.5",
		"postcss-simple-vars": "7.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.6.2",
		"rxjs": "7.8.0",
		"stylelint": "14.16.1",
		"stylelint-config-idiomatic-order": "9.0.0",
		"stylelint-config-prettier": "9.0.4",
		"stylelint-config-standard": "29.0.0",
		"vite": "4.0.4",
		"vite-tsconfig-paths": "4.0.3",
		"web-vitals": "3.1.1"
	},
	"devDependencies": {
		"@fastify/middie": "8.1.0",
		"@nft-marketplace/eslint-config": "workspace:*",
		"@nft-marketplace/tsconfig": "workspace:*",
		"@types/node": "16.11.7",
		"@types/nodemon": "1.19.2",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"nodemon": "2.0.20",
		"picocolors": "1.0.0"
	},
	"peerDependencies": {
		"tsx": "^3.0.0",
		"typescript": "^4.0.0"
	},
	"optionalDependencies": {
		"@storybook/addon-docs": "7.0.0-beta.28",
		"@storybook/addon-essentials": "7.0.0-beta.28",
		"@storybook/addon-highlight": "7.0.0-beta.28",
		"@storybook/addon-measure": "7.0.0-beta.28",
		"@storybook/addon-outline": "7.0.0-beta.28",
		"@storybook/addons": "7.0.0-beta.28",
		"@storybook/cli": "7.0.0-beta.28",
		"@storybook/react": "7.0.0-beta.28",
		"@storybook/react-vite": "7.0.0-beta.28",
