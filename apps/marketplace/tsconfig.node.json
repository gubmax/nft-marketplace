{
	"compilerOptions": {
		"target": "ESNext",
		"strict": true,
		"composite": true,
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"baseUrl": "."
	},
	"include": [
		".eslintrc.cjs",
		".storybook",
		"package.json",
		"plugins",
		"scripts",
		"server",
		"vite.config.ts"
	]
}
