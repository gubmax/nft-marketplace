{
  "name": "nft-marketplace",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "author": "Maksim Gubyshev <gubyshev@gmail.com> (https://github.com/gubmax)",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite-node server",
    "debug": "vite-node --inspect-brk server",
    "build": "pnpm build:client && pnpm build:render && pnpm build:server",
    "build:client": "vite build --outDir dist/client",
    "build:render": "vite build --ssr src/entry-server --outDir dist/server",
    "build:server": "vite-node scripts/build",
    "prerender": "vite-node scripts/prerender",
    "generate": "vite build --outDir dist/static && pnpm build:render && pnpm prerender",
    "serve": "cross-env NODE_ENV=production node dist/server"
  },
  "dependencies": {
    "@fastify/compress": "6.1.1",
    "@fastify/static": "6.5.0",
    "cross-env": "7.0.3",
    "fastify": "4.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@fastify/middie": "8.0.0",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@vanilla-extract/css": "1.7.3",
    "@vanilla-extract/vite-plugin": "3.3.1",
    "@vitejs/plugin-react": "2.0.1",
    "esbuild": "0.15.7",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vite-node": "0.23.0"
  },
  "overrides": {
    "@vanilla-extract/vite-plugin": {
      "vite": "^3"
    }
  }
}