{
  "name": "nft-marketplace",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "author": "Maksim Gubyshev <gubyshev@gmail.com> (https://github.com/gubmax)",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server",
    "dev": "tsx scripts/watch",
    "build": "pnpm build:client && pnpm build:render && pnpm build:server && pnpm prerender",
    "build:client": "vite build",
    "build:render": "vite build --ssr",
    "build:server": "tsx scripts/build",
    "prerender": "tsx scripts/prerender",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src scripts --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/compress": "6.1.1",
    "@fastify/static": "6.5.0",
    "cross-env": "7.0.3",
    "fastify": "4.8.1",
    "hyperid": "3.0.1",
    "pino": "8.6.1",
    "pino-pretty": "9.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.2"
  },
  "devDependencies": {
    "@fastify/middie": "8.0.0",
    "@types/node": "18.8.5",
    "@types/nodemon": "1.19.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "@vanilla-extract/css": "1.9.1",
    "@vanilla-extract/vite-plugin": "3.6.1",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.12",
    "esbuild": "0.15.11",
    "eslint": "8.25.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.20",
    "picocolors": "1.0.0",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "tsx": "3.10.1",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-tsconfig-paths": "3.5.1"
  }
}
