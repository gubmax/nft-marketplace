    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@fastify/middie": "8.0.0",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@vanilla-extract/css": "1.9.0",
    "@vanilla-extract/vite-plugin": "3.3.1",
    "@vitejs/plugin-react": "2.1.0",
    "esbuild": "0.15.7",
    "typescript": "4.8.2",
    "vite": "3.1.0",
